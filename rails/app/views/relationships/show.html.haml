- title "Relationship"

%p
  %strong Follower:
  = @relationship.follower
%p
  %strong Followee:
  = @relationship.followee
%p
  %strong Status:
  = @relationship.status

%p
  = link_to "Edit", edit_relationship_path(@relationship)
  |
  = link_to "Destroy", @relationship, :confirm => 'Are you sure?', :method => :delete
  |
  = link_to "View All", relationships_path
